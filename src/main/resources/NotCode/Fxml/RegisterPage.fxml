<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1030.0" prefWidth="1950.0" styleClass="anchor" stylesheets="@../Css/PageStylesheet.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.ViewController.RegisterPageController">
    <children>
        <TextField fx:id="usernameField" layoutX="699.0" layoutY="629.0" prefHeight="53.0" prefWidth="562.0"/>
        <Label layoutX="504.0" layoutY="631.0" prefHeight="58.0" prefWidth="143.0"
               style="-fx-background-color: #000000; -fx-effect: dropshadow(three-pass-box, black, 50, 0.6, 0, 0);"
               text="Username" textFill="WHITE">
            <font>
                <Font size="31.0"/>
            </font>
        </Label>
        <PasswordField fx:id="passwordField" layoutX="696.0" layoutY="762.0" prefHeight="53.0" prefWidth="560.0"/>
        <Label layoutX="502.0" layoutY="766.0" prefHeight="62.0" prefWidth="138.0"
               style="-fx-background-color: #000000; -fx-effect: dropshadow(three-pass-box, black, 50, 0.6, 0, 0);"
               text="Password" textFill="WHITE">
            <font>
                <Font size="31.0"/>
            </font>
        </Label>
        <Button fx:id="registerButton" layoutX="699.0" layoutY="898.0" mnemonicParsing="false"
                onAction="#handleRegister" prefHeight="56.0" prefWidth="105.0"
                style="-fx-background-color: #f4f4f4; -fx-effect: dropshadow(three-pass-box, white, 50, 0.6, 0, 0); -fx-cursor: hand;"
                text="Register">
            <font>
                <Font size="21.0"/>
            </font>
        </Button>
        <Label fx:id="resultMessage" layoutX="1273.0" layoutY="770.0"
               style="-fx-background-color: #000000; -fx-effect: dropshadow(three-pass-box, black, 50, 0.6, 0, 0) dropshadow(three-pass-box, black, 50, 0.6, 0, 0);"
               textFill="#d10707">
            <font>
                <Font size="27.0"/>
            </font>
        </Label>
        <Button fx:id="backButton" layoutX="854.0" layoutY="898.0" mnemonicParsing="false" onAction="#handleBack"
                prefHeight="56.0" prefWidth="105.0"
                style="-fx-background-color: #b9b9b9; -fx-effect: dropshadow(three-pass-box, #b9b9b9, 50, 0.6, 0, 0); -fx-cursor: hand;"
                text="Back" textFill="WHITE">
            <font>
                <Font size="21.0"/>
            </font>
        </Button>
        <Label layoutX="22.0" layoutY="10.0" prefHeight="64.0" prefWidth="281.0" text="Register Menu" textFill="WHITE">
            <font>
                <Font name="Baskerville Old Face" size="45.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
