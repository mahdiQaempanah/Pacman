<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1030.0" prefWidth="1950.0" style="-fx-background-color: black;"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.ViewController.ScoreBoardPageController">
    <children>
        <ImageView fitHeight="670.0" fitWidth="697.0" layoutX="116.0" layoutY="68.0">
            <image>
                <Image url="@../Image/king.png"/>
            </image>
        </ImageView>
        <Accordion layoutX="1133.0" layoutY="204.0"/>
        <Button layoutX="945.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="62.0"
                prefWidth="198.0" style="-fx-cursor: hand; -fx-background-color: blue;" text="back">
            <font>
                <Font size="33.0"/>
            </font>
        </Button>
        <Button layoutX="945.0" layoutY="182.0" mnemonicParsing="false" onAction="#reloadScoreBoard" prefHeight="74.0"
                prefWidth="200.0" style="-fx-background-color: green; -fx-cursor: hand;" text="reload" textFill="WHITE">
            <font>
                <Font size="29.0"/>
            </font>
        </Button>
        <HBox fx:id="scoreBoardView" layoutX="1166.0" layoutY="170.0"/>
    </children>
</AnchorPane>
